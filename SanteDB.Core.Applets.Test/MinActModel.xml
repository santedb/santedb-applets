<ViewModel xmlns="http://santedb.org/model/view" name="sampleact">
  <type type="IdentifiedData" all="true">
    <property name="id" behavior="never"/>
  </type>
  <type type="Act" all="true">
    <property name="id" behavior="never"/>
    <property name="relationship" all="false">
      <property name="HasSubject" all="false">
        <property name="relationshipType" all="false"/>
        <property name="relationshipTypeModel" all="true"/>
      </property>
      <property name="HasComponent" all="false">
        <property name="targetModel" all="true"/>
      </property>
      </property>
  </type>
</ViewModel>